pipeline {
	any agent	
		
	Stages {
		stage ('stage one') {
			steps {
			     echo "building package"
			     sh 'npm install'

                       } 

		} 

              stage ('stage two') { 
	      		steps {
			     sh "npm run start:dev &"
			     echo 'app successfully deployed'

				} 

	  		} 
		} 

	}







